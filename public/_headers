# Disable caching for editor HTML files to ensure updates are immediate
/editor/*.html
  Cache-Control: no-cache, no-store, must-revalidate
  Pragma: no-cache
  Expires: 0

# Allow caching for static assets
/css/*
  Cache-Control: public, max-age=31536000

/js/*
  Cache-Control: public, max-age=31536000

/images/*
  Cache-Control: public, max-age=31536000
